{
  "name": "eleventy-filter-npm-package-downloads",
  "version": "0.1.0",
  "description": "Filter for 11ty to show the number of downloads of a npm package.",
  "main": "index.js",
  "scripts": {
    "codecov": "codecov -t $CODECOV_TOKEN",
    "snyk": "snyk test",
    "test": "npm run test:ci",
